//Пользователь вводит с клавиатуры три координаты: x, y, z. Они соответствуют координатам вектора (x, y, z) с основанием в начале координат (0, 0, 0). 
//Напишите программу, которая вычисляет длину этого вектора и выводит полученный результат в консоль

#include "stdafx.h"
#include <iostream>
using namespace std;



int main()
{
	float x;
	float y;
	float z;

	{
		cin >> x >> y >> z;
		float length;
		length = sqrt(x*x + y*y + z*z);
		cout << length;
	}

	system("pause");
	return 0;
}
